<h1> All Articles </h1>

<ul id = "articles">
<% @articles.each do |article| %>
  <li>
    <%= link_to article.title, article_path(article), class: "article_title" %>
  </li>
<% end %>
<% if logged_in? %>
  <%= link_to "New Article", new_article_path, class: "button-link"%><br/><br/>
<% end %>
</ul>
