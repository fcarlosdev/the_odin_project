<h1>New Kitten</h1>

<%= form_for @kitten do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :age %>
  <%= f.select :age, options_for_select(@ages_options)%>
  <br><br>
  <%= f.label :cuteness %>
  <%= f.check_box :cuteness %>

  <%= f.label :softness %>
  <%= f.check_box :softness %>
  <br>
  <%= f.submit "Create" %>
<% end %>
