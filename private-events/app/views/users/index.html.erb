<h1>Users</h1>
<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-5">
    <ul>
      <% @users.each do |user| %>
      <li> <%= user.name %>
      <% if !current_user?(user) %>
          | <%= link_to "delete", user, method: :delete,
                                        data: { confirm: "You sure?" } %>
        <% end %>
        </li>
      <hr>
      <% end %>
    </ul>
  </div>
  <div class="col-md-3"></div>
</div>
