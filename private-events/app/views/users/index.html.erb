<h1>Users</h1>
<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-5">
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.name %></td>
            <td>
              <% if !current_user?(user) %>
                <%= link_to "delete", user, method: :delete,
                    data: { confirm: "You sure?" } %>
              <% end %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="col-md-3"></div>
</div>
